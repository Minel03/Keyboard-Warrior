; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Finals"
run/main_scene="res://MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

PromptList="*res://PromptList.gd"
GlobalSignals="*res://GlobalSignals.gd"
ClickSound="*res://ClickSound.tscn"
InfinitePromptList="*res://InfinitePromptList.gd"
PromptList2="*res://PromptList2.gd"
MusicManager="*res://MusicManager.tscn"

[display]

window/size/width=620
window/size/height=785
window/handheld/orientation="portrait"

[global]

pixxel=false
camera=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_environment="res://default_env.tres"
